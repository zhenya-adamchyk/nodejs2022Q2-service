generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id   String @id @default(uuid())
    version Int @default(autoincrement())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    login String
    password String
}
